Proceso multiplicacion_2_numeros_enteros
	definir num1,num2,num3 Como Entero;
	escribir "Multiplicación de dos números enteros";
	escribir "Ingrese el primer número: ";
	leer num1;
	escribir "Ingrese el segundo número: ";
	leer num2;
	num3=num1*num2;
	Escribir "El producto de ", num1, " por ", num2, " es: ", num3;
FinProceso
