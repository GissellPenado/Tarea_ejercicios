Proceso Conversion_dolares_a_cordobas
	Escribir "Conversión de dólares a córdobas";
	definir dolares, cambio, cordobas como real;
	escribir "Ingrese la cantidad en dólares: ";
	leer dolares;
	escribir "Ingrese el tipo de cambio: ";
	leer cambio;
	cordobas=dolares*cambio;
	escribir "Su cantidad en córdobas es: ", cordobas;
FinProceso
