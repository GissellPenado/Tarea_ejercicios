Proceso Conversion_cordobas_a_dolares
	escribir "Conversión de córdobas a dólares";
	definir cordobas, cambio, dolares como real;
	escribir "Ingrese la cantidad en córdobas: ";
	leer cordobas;
	Escribir "Ingrese el tipo de cambio: ";
	leer cambio;
	dolares=cordobas/cambio;
	escribir "Su cantidad en dólares es: ", dolares;
FinProceso
